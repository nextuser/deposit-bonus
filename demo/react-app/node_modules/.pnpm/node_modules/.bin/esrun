#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ljl/work/sui/hackathon-submission/nextuser/demo/react-app/node_modules/.pnpm/esrun@3.2.26/node_modules/esrun/node_modules:/home/ljl/work/sui/hackathon-submission/nextuser/demo/react-app/node_modules/.pnpm/esrun@3.2.26/node_modules:/home/ljl/work/sui/hackathon-submission/nextuser/demo/react-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ljl/work/sui/hackathon-submission/nextuser/demo/react-app/node_modules/.pnpm/esrun@3.2.26/node_modules/esrun/node_modules:/home/ljl/work/sui/hackathon-submission/nextuser/demo/react-app/node_modules/.pnpm/esrun@3.2.26/node_modules:/home/ljl/work/sui/hackathon-submission/nextuser/demo/react-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esrun/bin.js" "$@"
else
  exec node  "$basedir/../esrun/bin.js" "$@"
fi
